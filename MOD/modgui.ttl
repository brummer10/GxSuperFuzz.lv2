@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_sfp_#_sfp_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxsuperfuzz.html> ;
        modgui:stylesheet <modgui/stylesheet-gxsuperfuzz.css> ;
        modgui:screenshot <modgui/screenshot-gxsuperfuzz.png> ;
        modgui:thumbnail <modgui/thumbnail-gxsuperfuzz.png> ;
        modgui:brand "Guitarix" ;
        modgui:label "GxSuperFuzz" ;
        modgui:model "boxy" ;
        modgui:panel "1-select-2-knobs" ;
        modgui:color "sufu" ;
        modgui:knob "cairo" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "TONE" ;
            lv2:name "TONE" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "EXPANDER" ;
            lv2:name "EXPANDER" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "BALANCE" ;
            lv2:name "BALANCE" ;
        ] ;
    ] .
